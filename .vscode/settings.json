{
  // TODO: Still a straight dump from current settings; need cleanup.
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools",
    "editor.formatOnType": false
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cmake.configureOnOpen": false,
  "code-runner.enableAppInsights": false,
  "code-runner.executorMap": {
    "ahk": "autohotkey",
    "applescript": "osascript",
    "autoit": "autoit3",
    "bat": "cmd /c",
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "clojure": "lein exec",
    "coffeescript": "coffee",
    "cpp": "cd $dir && g++-12 $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "crystal": "crystal",
    "csharp": "scriptcs",
    "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
    "dart": "dart",
    "fortran": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran-modern": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran_fixed-form": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "FortranFreeForm": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fsharp": "fsi",
    "go": "go run",
    "groovy": "groovy",
    "haskell": "runhaskell",
    "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
    "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
    "javascript": "node",
    "julia": "julia",
    "kit": "kitc --run",
    "less": "cd $dir && lessc $fileName $fileNameWithoutExt.css",
    "lisp": "sbcl --script",
    "lua": "lua",
    "nim": "nim compile --verbosity:0 --hints:off --run",
    "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "ocaml": "ocaml",
    "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
    "perl": "perl",
    "perl6": "perl6",
    "php": "php",
    "powershell": "powershell -ExecutionPolicy ByPass -File",
    "python": "python -u",
    "r": "Rscript",
    "racket": "racket",
    "ruby": "ruby",
    "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
    "sass": "sass --style expanded",
    "scala": "scala",
    "scheme": "csi -script",
    "scss": "scss --style expanded",
    "shellscript": "bash",
    "sml": "cd $dir && sml $fileName",
    "swift": "swift",
    "typescript": "ts-node",
    "v": "v run",
    "vbscript": "cscript //Nologo"
  },
  "debug.allowBreakpointsEverywhere": true,
  "debug.console.fontSize": 10,
  "debug.console.historySuggestions": false,
  "debug.focusWindowOnBreak": false,
  "diffEditor.codeLens": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeLens": false,
  "editor.cursorStyle": "line",
  "editor.fontFamily": "Fira Code,JetBrains Mono, Monaco, Menlo, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 12,
  "editor.fontWeight": "normal",
  "editor.formatOnType": true,
  "editor.inlineSuggest.enabled": true,
  "editor.lineHeight": 15,
  "editor.lineNumbers": "on",
  "editor.minimap.enabled": false,
  "editor.minimap.showSlider": "always",
  "editor.renderControlCharacters": true,
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "editor.stickyTabStops": true,
  "editor.suggestSelection": "first",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "explorer.confirmDelete": false,
  "files.autoGuessEncoding": true,
  "git.confirmSync": false,
  "gitlens.gitCommands.skipConfirmations": [
    "fetch:command",
    "stash-push:command",
    "switch:command",
    "branch-create:command"
  ],
  "latex-workshop.bibtex-fields.sort.enabled": true,
  "latex-workshop.bibtex-format.handleDuplicates": "Highlight Duplicates",
  "latex-workshop.bind.altKeymap.enabled": true,
  "latex-workshop.latex.autoBuild.run": "never",
  "latex-workshop.latex.autoClean.run": "onBuilt",
  "latex-workshop.latex.clean.fileTypes": [
    "*~",
    "*.aux",
    "*.bbl",
    "*.blg",
    "*.dvi",
    "*.idx",
    "*.ind",
    "*.lof",
    "*.lot",
    "*.log",
    "*.out",
    "*.toc",
    "*.acn",
    "*.acr",
    "*.alg",
    "*.glg",
    "*.glo",
    "*.gls",
    "*.fls",
    "*.log",
    "*.fdb_latexmk",
    "*.snm",
    "*.synctex(busy)",
    "*.synctex.gz(busy)",
    "*.nav",
    "*.vrb"
  ],
  "latex-workshop.latex.recipe.default": "lastUsed",
  "latex-workshop.latex.recipes": [
    {
      "name": "True Lab-clone (ptex + pbibtex)",
      "tools": ["platex", "pbibtex", "platex", "pbibtex", "platex", "dvipdfmx"]
    },
    {
      "name": "Lab-no-bib (kanji support). ptex + pbibtex",
      "tools": ["platex", "platex", "platex", "dvipdfmx"]
    },
    {
      "name": "Single pdflatex (arXiv)",
      "tools": ["pdflatex"]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "args": [
        "-halt-on-error",
        "-file-line-error",
        "-interaction=nonstopmode",
        "-kanji=utf-8",
        "-synctex=1",
        "%DOCFILE%.tex"
      ],
      "command": "platex",
      "name": "platex"
    },
    {
      "args": ["%DOCFILE%.dvi"],
      "command": "dvipdfmx",
      "name": "dvipdfmx"
    },
    {
      "args": [
        "-halt-on-error",
        "-file-line-error",
        "-l",
        "-ot",
        "-kanji=utf-8",
        "-synctex=1",
        "%DOCFILE%.tex"
      ],
      "command": "ptex2pdf",
      "name": "ptex2pdf"
    },
    {
      "args": ["-synctex=1", "-interaction=nonstopmode", "%DOCFILE%"],
      "command": "lualatex",
      "name": "luatex"
    },
    {
      "args": ["%DOCFILE%", "-kanji=sjis"],
      "command": "pbibtex",
      "name": "pbibtex"
    },
    {
      "args": ["%DOCFILE%"],
      "command": "bibtex",
      "name": "bibtex"
    },
    {
      "args": ["%DOCFILE%"],
      "command": "makeglossaries",
      "name": "Make glossaries"
    },
    {
      "args": ["-synctex=1", "-interaction=nonstopmode", "%DOCFILE%"],
      "command": "pdflatex",
      "name": "pdflatex"
    },
    {
      "args": ["-synctex=1", "-interaction=nonstopmode", "%DOCFILE%"],
      "command": "xelatex",
      "name": "xelatex"
    }
  ],
  "latex-workshop.linting.chktex.enabled": true,
  "latex-workshop.view.pdf.viewer": "tab",
  "ltex.additionalRules.enablePickyRules": true,
  "ltex.statusBarItem": true,
  "markdown.extension.orderedList.autoRenumber": false,
  "markdown.preview.fontSize": 12,
  "markdown.preview.lineHeight": 1.3,
  "markdownlint.config": {
    "MD033": {
      "allowed_elements": ["details", "summary", "sub", "br", "img"]
    },
    "MD046": {
      "style": "consistent"
    }
  },
  "mdmath.delimiters": "brackets",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "outline.showVariables": false,
  "python.formatting.provider": "black",
  "python.linting.enabled": true,
  "python.linting.pylintCategorySeverity.convention": "Warning",
  "python.linting.pylintEnabled": true,
  "python.testing.pytestEnabled": true,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.fontFamily": "Fira Code,Monaco",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.inheritEnv": false,
  "terminal.integrated.tabs.enabled": true,
  "testing.autoRun.delay": 300,
  "update.mode": "manual",
  "vim.easymotion": true,
  "vim.handleKeys": {
    "<C-c>": false,
    "<C-d>": true,
    "<C-f>": false,
    "<C-s>": false,
    "<C-v>": false,
    "<C-z>": false
  },
  "vim.hlsearch": true,
  "vim.incsearch": false,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "after": [],
      "before": ["<leader>", "d"],
      "commands": [
        {
          "args": {
            "snippet": "${CURRENT_YEAR}${CURRENT_MONTH}${CURRENT_DATE}_${CURRENT_HOUR}${CURRENT_MINUTE}"
          },
          "command": "editor.action.insertSnippet"
        }
      ]
    }
  ],
  "vim.surround": true,
  "vsicons.dontShowNewVersionMessage": true,
  "window.openFoldersInNewWindow": "on",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": -1,
  "workbench.colorTheme": "Catppuccin Latte",
  "workbench.editor.languageDetection": false,
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.wrapTabs": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.iconTheme": "catppuccin-latte",
  "workbench.statusBar.visible": true,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modifications",
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  }
}
